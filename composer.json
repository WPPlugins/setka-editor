{
    "name": "setka/setka-editor",
    "description": "Setka Editor. Create beautiful posts with WordPress.",
    "type": "wordpress-plugin",
    "license": "GPLv2 or later",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Kolya Korobochkin",
            "email": "me@korobochkin.com"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "symfony/form": "^3.2.6",
        "symfony/validator": "^3.2.6",
        "symfony/twig-bridge": "^3.2.6",
        "symfony/framework-bundle": "^3.2.6",
        "symfony/http-foundation": "^3.2.6",
        "hanneskod/classtools": "^1.1",
        "symfony/polyfill-mbstring": "1.3.0",
        "twig/twig": "^1.33.0",
        "php": ">=5.5.9"
    },
    "autoload": {
        "files": [
            "source/libraries/polyfill-mbstring/bootstrap.php"
        ],
        "psr-4": {
            "Setka\\Editor\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setka\\Editor\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^2.9"
    }
}
